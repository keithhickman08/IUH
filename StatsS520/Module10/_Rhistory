bpRule <- function(x, const=1.5, positions=FALSE) {
x <- x[!is.na(x)]
qs <- quantile(x,probs = c(0.25,0.75))
iqr <- qs[2]-qs[1]
if (!positions) x[x < qs[1]-const*iqr | x > qs[2]+const*iqr]
else which(x < qs[1]-const*iqr | x > qs[2]+const*iqr)
}
